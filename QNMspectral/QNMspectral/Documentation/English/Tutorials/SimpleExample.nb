Cell[BoxData[{
 RowBox[{
  RowBox[{"gdd", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"-", 
        RowBox[{"f", "[", "u", "]"}]}], ",", 
       RowBox[{"-", 
