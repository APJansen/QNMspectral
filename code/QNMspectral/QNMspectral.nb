Cell["add grid and normalize", "Subsubsection",
 CellChangeTimes->{{3.686546528805954*^9, 3.6865465308030987`*^9}}],

Cell["\<\
First remove any possible copies due to higher powers of the frequency.
Then split the full eigenvector into separate eigenfunctions in case of \
multiple equations.
Finally add the grid and normalize to be 1 at the horizon.\
\>", "Text",
 CellChangeTimes->{{3.686548193235854*^9, 3.686548263270224*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"formatEigenfunctions", "[", 
   RowBox[{"eigensyst_", ",", "grid_", ",", "Neqs_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"order", "=", 
       RowBox[{
        RowBox[{"Length", "[", "grid", "]"}], "-", "1"}]}], ",", "freqs", ",",
       "evecs", ",", "evecsUnique", ",", "evecsSorted", ",", 
      "evecsNormalized", ",", "evecsGrid"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"freqs", ",", "evecs"}], "}"}], "=", 
      RowBox[{"Transpose", "[", "eigensyst", "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"evecsUnique", "=", 
      RowBox[{"Take", "[", 
       RowBox[{
        RowBox[{"Transpose", "[", "evecs", "]"}], ",", 
        RowBox[{"Neqs", " ", 
         RowBox[{"(", 
          RowBox[{"order", "+", "1"}], ")"}]}]}], "]"}]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"evecsSorted", "=", 
